# Database Configuration
# Use 5432 if it's free, or 5433 if 5432 is occupied
DB_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password123
POSTGRES_DB=mydb

# Flask Configuration
# Database URL (Make sure the port matches DB_PORT)
DATABASE_URL=postgresql://postgres:password123@localhost:5432/mydb
SECRET_KEY=dev_secret_key_change_in_production
JWT_SECRET_KEY=dev_jwt_secret_change_in_production

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
